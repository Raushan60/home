<script type="text/javascript">
    function makemesecure(txtuname, txtpwd, txttemp, txttemp2) {

        try {
            txtuname = document.getElementById('ctl00_BodyContent_UploadResume_txtUserName');
            txtpwd = document.getElementById('ctl00_BodyContent_UploadResume_txtPassword');
            txttemp = document.getElementById(txttemp);
            txttemp2 = document.getElementById(txttemp2);

            if (txtuname.value == '') {
                showPopup("Invalid credential");

                return false;
            }
            if (txtpwd.value == '') {
                showPopup("Invalid credential");
                return false;
            }

            var s = txtuname.value;
            txttemp2.value = Encrypt(s, '5AA9A8A0417065687E0B2484C04C7044BEE75BBC6EE6547048E8315AA25A5283A28875439B6C094BD8033197F51F19B636A6D57986A6E5D3D85072A5FB8AED73', '10001');
            var strStar = '';
            for (i = 0; i < 25; i++) { strStar = strStar + '*'; }
            txtuname.value = strStar;
            s = txtpwd.value;
            txttemp.value = Encrypt(s, '5AA9A8A0417065687E0B2484C04C7044BEE75BBC6EE6547048E8315AA25A5283A28875439B6C094BD8033197F51F19B636A6D57986A6E5D3D85072A5FB8AED73', '10001');
            var strStar = '';
            for (i = 0; i < 25; i++) { strStar = strStar + '*'; }
            txtpwd.value = strStar;
            if ((txtuname.value != '') && (txtpwd.value != '')) {
                //ShowProgress();
                return true;
            }
        }
        catch (ex) {
            showPopup(ex.message);
            return false;
        }
    }
    function EnableKeys(keyType, e) {
        var keyCode;
        if (window.event) //IE
            keyCode = event.keyCode;
        else if (e.which) //Netscape/Firefox/Opera
            keyCode = e.which;
        if (keyType == 555) {
            if (keyCode == 38)
                return false;
        }
    }
</script>

